.slick-prev,
.slick-next {
    position: absolute;
    display: block;
    height: 31px;
    width: 31px;
    line-height: 0px;
    font-size: 0px;
    cursor: pointer;
    background: transparent;
    color: transparent;
    top: 47%;
    margin-top: -10px;
    padding: 0;
    border: none;
    outline: none;
    &:hover{
        outline: none;
        background: transparent;
        color: transparent;
        &:before {
            opacity: $slick-opacity-on-hover;
			color: $link-color-hover;
			
        }
    }
    &.slick-disabled:before {
        opacity: $slick-opacity-not-active;
    }
}

.slick-prev:before, .slick-next:before {
    font-family: $fa;
    font-size: 30px;
    line-height: 1.2em;
    color: $fa-off-color;
    opacity: $slick-opacity-default;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
	&:hover {
		color: $link-color-hover;
	}

	
}
.slick-track {  padding: 0px 0 12px;}
.slick-prev {
    left: -10px;
	color: #ccc;
    [dir="rtl"] & {
        left: auto;
        right: -0;
    }
    &:before {
        content: $slick-prev-character;
        [dir="rtl"] & {
            content: $slick-next-character;
        }
    }
}

.slick-next {
    right: -10px;
	
    [dir="rtl"] & {
        left: 0;
        right: auto;
    }
    &:before {
        content: $slick-next-character;
        [dir="rtl"] & {
            content: $slick-prev-character;
        }
    }
}

/* Dots */

.slick-slider {
    margin-bottom: 4px;
}

.slick-dots {
    position: absolute;
    bottom: -32px;
    list-style: none;
    display: block;
    text-align: center;
    padding: 0;
    width: 100%;
    li {
        position: relative;
        display: inline-block;
        height: 20px;
        width: 20px;
        margin: 0 5px;
        padding: 0;
        cursor: pointer;
        button {
            border: 0;
            background: transparent;
            display: block;
            height: 20px;
            width: 20px;
            outline: none;
            line-height: 0px;
            font-size: 0px;
            color: transparent;
            padding: 5px;
            cursor: pointer;
            &:hover {
                outline: none;
                &:before {
                    opacity: $slick-opacity-on-hover;
					color: $link-color-hover;
                }
            }
            &:before {
                position: absolute;
                top: 0;
                left: 0;
                content: $slick-dot-character;
                width: 20px;
                height: 20px;
                font-family: $fa;
                font-size: $slick-dot-size;
                line-height: 20px;
                text-align: center;
                color: $fa-off-color;
                opacity: $slick-opacity-not-active;
                -webkit-font-smoothing: antialiased;
                -moz-osx-font-smoothing: grayscale;
            }
        }
        &.slick-active button:before {
            color: $slick-dot-color-active;
            opacity: $slick-opacity-default;
        }
    }
}

.content-carousel {
	.slick-dots {		
		li {			
			button {				
				&:hover {					
					&:before {
						opacity: $slick-opacity-on-hover;
						color: $link-color-hover;
					}
				}
				&:before {
					color: #ccc;
					
				}
			}
			&.slick-active button:before {
				color: $slick-dot-color-active;
				opacity: $slick-opacity-default;
			}
		}
	}	
}




