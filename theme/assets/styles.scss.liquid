/********************************************
 * 	CART
 *******************************************/
.cart-table{
	tfoot td { text-align: right;}
	margin-bottom: 20px !important;
	.cart-row {
		td {padding-top: 16px;}
		&:first-child td {padding-top: 24px;}
		.cart-line-item{
			padding-right: 24px;
			
			vertical-align: top;
			text-align: center;
		}
		.cart-item {
			h3 {
				padding: 0;				
				color: $link-color;
				@include font-size(1.8);
				&:hover { color: $link-color-hover;}
			}
			.cart-item-title {
				margin-left: 4px;
				@include font-size(1.4);
			}
			.itemImage {max-width: 100%;}
			.cart-subtitle {margin: -12px 0 12px;;}
			
		}
		.cart-price {
			color: $body-color;		
			
		}
		.cart-qty {
					
			width: 110px;			
			@include font-size(1.6);
			input {
				font-weight: bold; padding: 6px;
				height: 40px;
				text-align: center;				
				width: 40px;
				display: inline;
				@include font-size(1.6);
			}
			> div {
				padding: 0 0 0 17px;
			}
			.cart-update-clone {
				padding: 9px ;
				width: 40px;

			}
			

			i{			
				
				color: #fff;				
				@include font-size(1.5);
				&:hover {color: #fff;}
				
				
			}
		}
		.cart-total {
			color: $body-color;
			
		}
		.cart-delete {
			text-align: center;
			i {
				color: #ccc;
				@include font-size(2.4);
				&:hover {color: $link-color-hover;}
				
			}
		}
	&.cart-product-option {
		background-color: #ddd;
		h3 {
			color: $body-color;
			margin-bottom: 0;
			&:hover {color: $body-color;}
		}
		.cart-item {
			i {
				@include font-size(1.4);
				margin: 0 12px 0 0;;
			}
		}
		
		td {padding-top: 8px  ; padding-bottom: 8px;}
		}
	}
	@media screen and (max-width: $screen-sm-max) {
		th,td {@include font-size(1.3);}
		
		.cart-row {
			
			.cart-item {
				max-width: 100px;
				white-space: normal;
				h3 {@include font-size(1.4); white-space: normal;}
				.itemImage {max-width: 30%;}

				
				
			}
			.cart-qty{
				width: 80px;			
				@include font-size(1.6);
				input {
					font-weight: normal; padding: 6px;
					height: 30px;								
					width: 30px;
					
					@include font-size(1.4);
				}
				> div {
					padding: 0 0 0 17px;
				}				
				
				
				.cart-update-clone {
					padding: 4px 2px;
					width: 30px;
					
				}
			}
		}		
	}
	@media screen and (max-width: $screen-xs-max) {
		td {@include font-size(1.2);}
		th {@include font-size(1.1);}
		tfoot td { text-align: center;}
		.cart-row {
			.cart-item {
				
				.itemImage {max-width: 80%;}
				i {display: none;}
				.fa-ul {margin-left: 0;}				
			}
		}
		
	} 
}

