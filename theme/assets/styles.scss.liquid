            color: #fff;
            cursor: pointer;
            line-height: 1;
			padding-top: 12px;
            .icon {
                float: right;
                em {
                    background: none repeat scroll 0 0 $orion-bar-arrow-color;
                    display: block;
                    height: 2px;
                    margin-bottom: 3px;
                    width: 20px;                      
                }
            }            
            span.title { float: left;  margin: 0; color: $orion-link-color;}            
		}
		&.active > a {font-weight: bold;}
		a {
			text-decoration: none; 
			display: block; 
			@include transition-color() ;
			&:hover {text-decoration: underline;}
		}
	}
