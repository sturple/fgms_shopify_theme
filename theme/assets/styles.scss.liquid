.panel-heading { border-top-left-radius: 0; border-top-right-radius: 0; -moz-border-top-right-radius: 0px; -webkit-border-top-right-radius: 0px; -moz-border-top-left-radius: 0px; -webkit-border-top-left-radius: 0px;}
.panel-title { padding-top: 0; }

/* popovers */
.popover-content label { font-size: 11px !important; }
.popover-content select { width: 100%; }
.popover-content .form-group { margin-bottom: 6px !important; padding:0  0 0 15px; }
.popover-title {border-radius: 0px;	-moz-border-radius: 0px; -webkit-border-radius: 0px;}
.popover {border-radius: 0px;	-moz-border-radius: 0px; -webkit-border-radius: 0px;}

/* Modal */
.modal { top : 100px;}
.modal-body {  max-height: 610px;  overflow: auto;}
.meetings-modal .modal-body { max-height: none; }
.modal-body h2 { padding-top: 0px;}



.btn-default, .summit {
	border: $btn-default-border;
	font-family: $btn-font-family;
	color: $btn-default-color-hover;	
	@include btn-template($btn-default-color,$btn-default-background, $btn-default-padding,$btn-default-radius,$btn-default-box-shadow);
	border: 1px solid #ccc;
	&:hover, &:focus, &:active, &.active, &.disabled, &[disabled] {
		@include btn-template-hover($btn-default-background-hover);
		color: $btn-default-color-hover;
		}
	@media screen and (max-width: $screen-lg-max) {padding: $btn-primary-padding-lg;}
	@media screen and (max-width: $screen-md-max) {padding: $btn-primary-padding-md; @include font-size($btn-primary-font-size-md);}
	@media screen and (max-width: $screen-sm-max) {padding: $btn-primary-padding-sm;}
	
}



.btn-primary, input[type="submit"]{
	font-family: $btn-font-family;
	
	@include btn-template( $btn-primary-color,$btn-primary-background, $btn-primary-padding,$btn-primary-radius,$btn-primary-box-shadow);
	

	&:hover, &:focus, &:active, &.active, &.disabled, &[disabled] {
		@include btn-template-hover($btn-primary-background-hover);
		color: $btn-primary-color-hover;
	}
	@media screen and (max-width: $screen-lg-max) {padding: $btn-primary-padding-lg;}
	@media screen and (max-width: $screen-md-max) {padding: $btn-primary-padding-md; @include font-size($btn-primary-font-size-md);}
	@media screen and (max-width: $screen-sm-max) {padding: $btn-primary-padding-sm;}
	
}

.btn-secondary {
	font-family: $btn-font-family;
	@include btn-template($btn-secondary-color,$btn-secondary-background, $btn-secondary-padding,$btn-secondary-radius,$btn-secondary-box-shadow);
	border: 1px solid #9ad7d2;
	&:hover, &:focus, &:active, &.active, &.disabled, &[disabled] {
		@include btn-template-hover($btn-secondary-background-hover)	;
		 color: $btn-secondary-color-hover;
		}
	@media screen and (max-width: $screen-lg-max) {padding: $btn-primary-padding-lg;}
	@media screen and (max-width: $screen-md-max) {padding: $btn-primary-padding-md; @include font-size($btn-primary-font-size-md);}
	@media screen and (max-width: $screen-sm-max) {padding: $btn-primary-padding-sm;}
	
}

.btn-lg, .btn-group-lg > .btn {  padding: $btn-lg-padding; @include radius($btn-primary-radius);}
.btn-sm, .btn-group-sm > .btn {  padding: $btn-sm-padding; @include radius($btn-primary-radius);}
.btn-xs, .btn-group-xs > .btn {  padding: $btn-xs-padding; @include radius($btn-primary-radius);}

.btn-header {
    padding: 12px 48px;
    line-height: 2.7em;
    color: #fff !important;
    background-color:#2181a4;
    border-left: 1px solid #8cb8c9;
    border-right: 1px solid #8cb8c9;
    margin-left: -3px;
    margin-right: -3px;
    white-space: nowrap;
    
    &:hover{
         color: #fff;
         text-decoration: underline;
       
        
    }
   	@media screen and (max-width: $screen-lg-max) {}
	@media screen and (max-width: $screen-md-max) {padding:12px 24px;}
	@media screen and (max-width: $screen-sm-max) {padding:12px 18px;}
	@media screen and (max-width: $screen-xs-max) {white-space: normal;display: block; margin: 4px 0; text-align: center; line-height: 1.4em;} 
}





