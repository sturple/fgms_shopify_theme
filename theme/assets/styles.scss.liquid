	z-index: 3;
	
	.cart-account-links  { display: none; visibility: hidden;}
	.column-wrapper {
		padding-top: 8px;
		padding-bottom: 8px;
	 
    }
	.orion-menu > li > a {
		line-height: $fixed-header-with-scroll-height;
		padding-top: 0;
		padding-bottom: 0;
		text-transform: uppercase;
		

		-webkit-transition:0.3s all ease 0s;
		-moz-transition:0.3s all ease 0s;
		-ms-transition:0.3s all ease 0s;
		-o-transition:0.3s all ease 0s;
		transition:0.3s all ease 0s;		
	}
	.header-logo img {
		height: $fixed-header-with-scroll-height;
		position: relative;
		-webkit-transition:1s all cubic-bezier(0.23, 1, 0.32, 1);
		-moz-transition:1s all cubic-bezier(0.23, 1, 0.32, 1);
		-ms-transition:1s all cubic-bezier(0.23, 1, 0.32, 1);
		-o-transition:1s all cubic-bezier(0.23, 1, 0.32, 1);
		transition:1s all cubic-bezier(0.23, 1, 0.32, 1);		
		
	}
	&.script-fixed-header-with-scroll{
		position: relative;
		display: table;
		width: 100%;		
		-webkit-transition: top .25s ease;
		 transition: top .25s ease;
		 -webkit-transition: all top .25s ease ease-out;
		 -moz-transition: all top .25s ease ease-out;
		 transition: all top .25s ease ease-out;
		 -webkit-transition: top .25s ease .2s ease-out;
		 -moz-transition: top .25s ease .2s ease-out;
		 -o-transition: top .25s ease .2s ease-out;
		 transition: top .25s ease .2s ease-out;
		&.navbar-static-top {
			top: -5em;
			&.shrink {
				border-bottom: $header-border-bottom;
				.orion-menu > li > a {line-height: $fixed-header-with-scroll-height-shrink;}
				.header-logo img {
					height: $fixed-header-with-scroll-height-shrink;
					
					
				}
				.cart-account-links  { display: block; visibility: visible;}
				.column-wrapper {
					padding-top: 4px;
					padding-bottom: 4px;
				 
				}					
			}
		
		}
		&.show {top: 0;	position: fixed;}
		
		
	}

	&.option2 {
		.header-nav-wrapper{float: right;}
		.header-utility-wrapper {float: right; margin-left: 24px;}
	}

	.social-icons {
	    i {
	        color: #fff;
	        line-height: 2.2em;
	        @include font-size(1.8);
	        margin-left: 8px;
	        @include transition();
	     
	        &:hover {color: #9ad7d2;}
	    
	    }
	}
	
    @media screen and (max-width: $screen-lg-max) {
       
    }
    @media screen and (max-width: $screen-md-max) {
		&.option2 {
			.header-nav-wrapper{float: left; margin-left: -15px}
			
		}       
    }
    @media screen and (max-width: $screen-sm-max) {
        text-align: center !important;
        .social-icons {text-align: center !important;}
		&.option2 {
			.header-nav-wrapper{float: none;margin-left:0;}
			.header-utility-wrapper {float: right; margin-left: 24px;}
		}
		.column-wrapper {
				 
		}
        .header-global-html {text-align: center;}
        .header-logo {
            text-align: center;
			margin-bottom: 8px;
            img {display: inline;}
        }		
    }
    @media screen and (max-width: $screen-xs-max) {
