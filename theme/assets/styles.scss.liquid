	  &:hover {	color: $orion-link-color-hover; text-decoration: none; }
	}
	> li.has-indicator > a {
	        	&:after {
        	    font-family: FontAwesome;
        	    content: $orion-indicator;
        	    padding-left: 4px;
            }
	}	
	ul, ul li ul {
		list-style: none;   
		margin: 0;   
		padding: 12px 0; 
		display: none;    
		position: absolute;    
		z-index: 999;	
		width: $orion-submenu-width;
