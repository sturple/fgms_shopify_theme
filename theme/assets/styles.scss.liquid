/********** Orion Menu *******************/
#navigation-placeholder-wrapper{
	height: $navigation-placeholder-wrapper-height;

}


#navigation-wrapper {
  position: static;
  z-index: 999;
  top: $navigation-wrapper-top;
  
}


#navigation-orion {display: table;}
.orion-menu {
	
	padding: $orion-padding;
	background-color: $orion-background;
	margin: 0 ;
	position: relative; 
	width: auto;	
	list-style: none;
	z-index: 1000;

	> li {
		margin: 0;	
		padding: 0;	
		float: left; 
		position: relative;
		border-bottom: $orion-bar-height solid transparent;
		&:hover {}
		&:last-child a {padding-right: 0;}		
		&.showhide {
            color: $orion-link-color;
            cursor: pointer;
            line-height: 1;
			padding-top: 12px;
            .icon {
                float: right;
                em {
                    background: none repeat scroll 0 0 $orion-link-color;
                    display: block;
                    height: 2px;
                    margin-bottom: 3px;
                    width: 20px;                      
                }
            }            
            span.title { float: left;  margin: 0; color: $orion-link-color;}            
		}
		&.active > a {font-weight: bold;}
		a {
			text-decoration: none; 
			display: block; 
			@include transition-color() ;
			&:hover {text-decoration: underline;}
		}
	}
	> li  > a { 
	  color: $orion-link-color; 
	  &:hover {	color: $orion-link-color-hover; text-decoration: none; }
	  
	}
	> li.has-indicator > a {
	        	&:after {
        	    font-family: FontAwesome;
        	    content: $orion-indicator;
        	    padding-left: 4px;
            }
	}	
	ul, ul li ul {
		list-style: none;   
		margin: 0;   
		padding: 12px 0; 
		display: none;    
		position: absolute;    
		z-index: 999;	
		width: $orion-submenu-width;
	}
	ul {
		top: $orion-submenu-top; 
		left: 0; 
		@include font-size($orion-submenu-font); 
		@include box-shadow(2px 2px 4px rgba(0, 0, 0, 0.35)); 
		background: $orion-submenu-background;
		background-color: #fff;
		text-align: left;
	
		li {
		    
			clear:both;
			width:100%;  
			a {
				width:100%; 
				padding:$orion-link-padding;	
				text-decoration: $orion-submenu-link-text-decoration;
				color: $orion-submenu-link-color;
				display:block; 
				float:left; 
				clear:both; 
				box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box;
			    &:before{
			            /*content="\f0da";*/
			            font-family: FontAwesome;
			            padding-right: 4px;
			            color: #fff;
			    
			    }				
				&:hover{
				    
					text-decoration: $orion-submenu-link-text-decoration-hover;
					color: $orion-submenu-link-color-hover;
				}
				&:before {padding-right: 8px;}
			}
			&:hover {
			    a{
			        &:before{
			           color: $orion-submenu-link-color-hover;			    
					}
			    }
			}
		}
	}
	ul li ul {top: 0; left: $orion-submenu-width;}
	> li {
		.showhide {
			display: none;	
			width: 100%; 
			cursor: pointer;	
			height: 44px; 	
			line-height: 1;
			span.title{
				margin: 16px 0 0 0;	
				float: left; 
				color: $orion-link-color;
			}
			span.icon{
				margin: 17px 0;	
				float: right;
			}
			.icon em{	
				margin-bottom: 3px;	
				display: block;	
				width: 20px; 
				height: 2px; 
				background: $orion-bar-arrow-color;
			}
		}
		> a {
		
			padding: $orion-link-padding; 
			@include font-size($orion-font) ;
			
			
		}
		/**** Sub menu link colors ****/
		> ul > li {
		  a {
			  color: $orion-submenu-link-color; 
			  padding: 4px 22px;
              text-decoration: $orion-submenu-link-text-decoration;       
			  &:hover {
				text-decoration: $orion-submenu-link-text-decoration-hover; 
				color: $orion-submenu-link-color-hover;				
			  }			
		  }
		  &.active > a {
				font-weight: bold;
		  }
	  }
	  
	  
	  
	}
	
    @media screen and (max-width: $screen-lg-max) {
        padding: $orion-padding-lg;
        float: none;
        > li {
        	> a { padding: $orion-link-padding-lg ; @include font-size($orion-font-lg);}
        }
        .indicator{right: 4px;top: 24px;}
    }
    @media screen and (max-width: $screen-md-max) {
        padding: $orion-padding-md;
		> li {
			> a {padding: $orion-link-padding-md; @include font-size($orion-font-md);}
	   }		
	   .indicator{right: 4px;top: 12px;}
      
    }
    @media screen and (max-width: $screen-sm-max) {
        padding: $orion-padding-sm;
	    width: auto;
	    margin: 0; 
	    display: block;
	    float: none;
	    //visibility: hidden;
	    
	    ul { 
	        top: 0;
	        @include box-shadow(none); 
	        background: transparent; 
	    
	    }
	    li{
	        width: 100%; 
	        border-bottom: 0px; 
	        &:hover {  border-bottom: 0px;}
	    }
	    > li > a {
	        padding: $orion-link-padding-sm; 
	        @include font-size($orion-font-sm); 
	        text-align: left;	
	        border-top: solid 1px rgba(255, 255, 255, 0.05);
			
	        box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box;
	    }
        li > ul > li a {
           color: $orion-link-color; 
           padding: 4px 12px;text-align: left;
           &:hover {
               text-decoration: underline; 
               color: $orion-link-color-hover;
            }
        }
        a{width: 100%;	box-sizing:border-box;	-moz-box-sizing:border-box; -webkit-box-sizing:border-box; 	}
        ul, ul li ul{
            width: 100%; 
            left: 0; 
            padding: 0 12px; 
            position: relative;	
            box-sizing:border-box;	
            -moz-box-sizing:border-box; 		
            -webkit-box-sizing:border-box; 
        }	
	    ul li .indicator{display: block;}
	    > li.showhide{display: block;}
        > li:hover  { border-bottom: 0px;}       
    }
    @media screen and (max-width: $screen-xs-max) {
        padding: $orion-padding-xs;
    }   	
	
}


