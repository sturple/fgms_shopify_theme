/*1200*/
@media screen and (max-width: $screen-lg-max) {
	.align-right-lg {text-align: left;}
	/** Footer **/
	#socialLinks {  padding: 12px 0;}
	#paymentMethods { text-align: left;}
	.payment-column-special-notes-column{padding-left: 36px;}
}
/*980*/
@media screen and (max-width: $screen-md-max) {
	body , .form-control{font-size: 15px;}
	#contentWrapper {margin: 18px 0 42px;}
	.header-align-right, #imageLogo ,.header-cart-bar{  text-align: center;}	
	.align-right-md {text-align: left;}
}
	
/*768*/
@media screen and (max-width: $screen-sm-max) {
	.control-label {margin-bottom: 4px; padding-left: 19px;}
	.nav-tabs.accordion { display:none; }
	.tab-content.accordion .tab-accordion-header {
		display:block;
		border:1px solid #f5f4f3;
		padding:8px 12px;
		margin-top:4px;
		webkit-box-shadow:1px 1px 2px rgba(0,0,0,0.15);
		-moz-box-shadow:1px 1px 2px rgba(0,0,0,0.15);
		box-shadow:1px 1px 2px rgba(0,0,0,0.15);
	}
	.hidden-768 {display: none; visibility: hidden;}
  	.tab-content.accordion .tab-pane {border:1px solid #ccc; padding:24px 12px;	background-color:transparent;	}align-right-sm {text-align: left;}	
	.btn-lg, .btn-group-lg > .btn {
		@include font-size(1.8);		
		padding: 12px 36px;
	  }
	.table-responsive {  margin-bottom: 36px;}

	
}
/*480*/
@media screen and (max-width: $screen-xs-max) {
	.display-block-480 {display: block; margin-bottom: 4px;}
	.display-none-480 {display: none; visibility: hidden;}
	.align-right-xs {text-align: left;}
	.pill-wrapper.form-selector .btn {padding: 4px 6px;  }
	.social-stream-wrapper { margin: 0 auto; width: 268px; }
	#social-stream { padding: 24px;}
	.dcsns-toolbar {  overflow: visible;}
	.dcsns-bottom-shadow {
		background: url({{'background-social-feeds-shadow.gif' | asset_url}}) no-repeat center bottom;; 	  
		bottom: 29px;		
		height: 11px;
		left: 19px;
		position: absolute;
		width: 93%;
	}
		
			
	
}



