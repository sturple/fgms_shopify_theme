/** gallery overlay smoothzoom **/

.sz-overlay {
  position:fixed;
  z-index:9998;
  display:none;
  top:0;
  left:0;
  width:100%;
  height:100%;
  background-color:#000;
  -moz-opacity:0.95;
  -khtml-opacity:0.95;
  -webkit-opacity:0.95;
  opacity:0.95;
  -ms-filter:progid:DXImageTransform.Microsoft.Alpha(opacity=95);
  filter:alpha(opacity=95);
  -webkit-transform:translateZ(0);
}
.sz-zoomed {
  z-index:9999;
  display:block;
  text-decoration:none;
  background-size:contain !important;
  background-position:center !important;
  background-repeat:no-repeat !important;
  position:fixed;
  cursor:pointer;
}
.sz-caption {
  z-index:9999;
  position:fixed;
  bottom:0;
  width:100%;
  background:#000;
  color:#fff;
  text-align:center;
  display:none;
}
.sz-caption span {
  z-index:9999;
  display:inline-block;
  padding:5px;
  font-size:12px;
  letter-spacing:.2em;
  text-transform:uppercase;
}
.sz-close {
  z-index:9999;
  position:fixed;
  top:5px;
  right:5px;
  padding:9px;
  color:#fff;
  text-decoration:none;
  font-size:34px;
  border-radius:42px;
  display:none;
}
.sz-left, .sz-right {
  z-index:9999;
  position:fixed;
  top:50%;
  left:0;
  margin-top:-25px;
  text-decoration:none;
  height:80px;
  line-height:80px;
  width:3%;
  -moz-opacity:0.5;
  -khtml-opacity:0.5;
  -webkit-opacity:0.5;
  opacity:0.5;
  -ms-filter:progid:DXImageTransform.Microsoft.Alpha(opacity=50);
  filter:alpha(opacity=50);
  background:#fff;
  color:#666;
  text-align:center;
  padding-top:4px;
  border-radius:0 40px 40px 0;
  -moz-border-radius:0 40px 40px 0;
  display:none;
}
.sz-left i, .sz-right i {
  font-size:28px;
  font-size:2.8rem;
}
.sz-right {
  right:0;
  left:auto;
  border-radius:40px 0 0 40px;
  -moz-border-radius:40px 0 0 40px;
}
@media screen and (max-width: 414px) {
  .sz-left, .sz-right { width:50px; }
}


/********** Anaimation **********/

.animated {
  -webkit-animation-duration: 1s;
  animation-duration: 1s;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
}

.animated.infinite {
  -webkit-animation-iteration-count: infinite;
  animation-iteration-count: infinite;
}

.animated.hinge {
  -webkit-animation-duration: 2s;
  animation-duration: 2s;
}

@-webkit-keyframes slideInLeft {
  from {
	@include transform(translate3d(-25%, 0, 0));
    visibility: visible;
  }
  to {@include transform(translate3d(0, 0, 0)); }
}

@keyframes slideInLeft {
  from {
    @include transform(translate3d(-25%, 0, 0));    
    visibility: visible;
  }
  to { @include transform(translate3d(0, 0, 0)); }
}

.slideInLeft {
  -webkit-animation-name: slideInLeft;
  animation-name: slideInLeft;
}

@-webkit-keyframes slideInRight {
  from {
	@include transform(translate3d(25%, 0, 0));
    visibility: visible;
  }
  to {@include transform(translate3d(0, 0, 0)); }
}

@keyframes slideInRight {
  from {
    @include transform(translate3d(25%, 0, 0));    
    visibility: visible;
  }
  to { @include transform(translate3d(0, 0, 0)); }
}

.slideInRight {
  -webkit-animation-name: slideInRight;
  animation-name: slideInRight;
}

@-webkit-keyframes slideInUp {
  from {
    @include transform(translate3d(0, 50%, 0));   
    visibility: visible;
  }
  to { @include transform(translate3d(0, 0, 0)); }
}

@keyframes slideInUp {
  from {
    @include transform(translate3d(0, 50%, 0));   
    visibility: visible;
  }
  to { @include transform(translate3d(0, 0, 0)); }
}

.slideInUp {
  -webkit-animation-name: slideInUp;
  animation-name: slideInUp;
}



@-webkit-keyframes zoomIn {
  from {
    @include opacity(0);
	@include transform(scale3d(.3, .3, .3))
  }

  50% {
    @include opacity(1);
  }
}

@keyframes zoomIn {
  from {
    @include opacity(0);
	@include transform(scale3d(.3, .3, .3))
  }

  50% {
    @include opacity(1);
  }
}

.zoomIn {
  -webkit-animation-name: zoomIn;
  animation-name: zoomIn;
}

@-webkit-keyframes fadeIn {
