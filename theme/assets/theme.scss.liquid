				align-items: center;
				justify-content: center;
				position: absolute;
				height: inherit;
				top: 0;
				width: 100%;
				left: 0;
				text-align: center;
				.gallery-social,
				.gallery-expander {
					@include font-size(2.4);
					a {
						@include opacity(0.8);
						margin: 0px 8px;
						@include font-size(2.4);
						&:hover {
							@include opacity(1);
						}
					}
				}
				.gallery-title {
					padding-top: 12px;
					font-weight: bold;
				}
			}
		}
		&:hover {
			.__st_gallery_overlay {
				@include transition(1s);
				@include opacity(1);
			}
			img {
				@include transform-scale(1.1, 1.1);
				@include transition(0.75s);
			}
		}
		&.active {
			.__st_gallery_overlay {
				@include opacity(1);
				i {
					display: none;
				}
			}
		}
	}
