.carousel-fade {
    .carousel-inner {
        .item {
            transition-property: opacity;
        }
        
        .item,
        .active.left,
        .active.right {
            @include opacity(0);
        }

        .active,
        .next.left,
        .prev.right {
            @include opacity(1);
        }

        .next,
        .prev,
        .active.left,
        .active.right {
            left: 0;
