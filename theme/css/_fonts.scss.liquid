@font-face {
	font-family: 'Overlock';
	font-style: italic;
	font-weight: 400;
	src: url({{ 'overlock-400-italic.eot' | asset_url }} );
	src: local('Overlock-Italic'),
    url({{ 'overlock-400-italic.woff2' | asset_url }} ) format('woff'),
    url({{ 'overlock-400-italic.woff' | asset_url }} ) format('woff'),
    url({{ 'overlock-400-italic.ttf' | asset_url }} ) format('truetype'),
    url({{ 'overlock-400-italic.svg#Overlock' | asset_url }} ) format('svg');
}
@font-face {
    font-family: 'Roboto';
    font-style: normal;
    font-weight: 400;
    src: url( {{ 'roboto-400-normal.eot' | asset_url }} );
    src: local('Roboto'), local('Roboto-Regular'),
    url( {{ 'roboto-400-normal.woff2' | asset_url }} ) format('woff'),
    url( {{ 'roboto-400-normal.woff' | asset_url }} ) format('woff'),
    url( {{ 'roboto-400-normal.ttf' | asset_url }} ) format('truetype'),
    url( {{ 'roboto-400-normal.svg#Roboto' | asset_url }}  ) format('svg');
}
@font-face {
    font-family: 'Roboto';
    font-style: normal;
    font-weight: 700;
    src: url( {{ 'roboto-700-normal.eot' | asset_url }} );
    src: local('Roboto Bold'), local('Roboto-Bold'),
    url( {{ 'roboto-700-normal.woff2' | asset_url }} ) format('woff'),
    url( {{ 'roboto-700-normal.woff' | asset_url }} ) format('woff'),
    url( {{ 'roboto-700-normal.ttf' | asset_url }} ) format('truetype'),
    url( {{ 'roboto-700-normal.svg#Roboto' | asset_url }}  ) format('svg');
}

