.scroll-button {
    position: absolute;
    width: 100%;
    &.top-button {
        bottom: 0;
        a {
            @include radius(10px 10px 0 0);
            //@include radius(0 0 10px 10px);
        }
        span {
            &::after {
                content: '\f106';
            }
            &:hover {            
                &::after {
                    content: 'Top';
                }
            }
        }
    }
    &.next-button {
      bottom: 0;
      a {
            @include radius(10px 10px 0 0);
      }
        span {
            &::after {
                content: '\f107';
            }
            &:hover {            
                &::after {
                    content: 'Scroll Down';
                }
            }
        }      
    }    
    z-index: 10;    
    a {
        width: 100px;
        margin: 0 auto;
        height: 40px;
        display: block;
        background-color : rgba(255,255,255, 0.9);
        text-align: center;
        &:hover {
            border-bottom: none !important;
        }
        
    }
    span{
        padding: 6px;
        display: block;
        &::after {            
            font-family: FontAwesome;
            font-size:  2.2em;
            color: #666 !important;

        }
        &:hover {
            padding: 3px;
            &::after {                
                text-transform: uppercase;
                font-family: inherit;
                font-size: 0.5em;
                color: #666 !important;
            }
        }        
    }
    
}

