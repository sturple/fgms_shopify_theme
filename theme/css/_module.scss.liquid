ul.collections-list {
    padding-top: 36px;
    li {
        margin-bottom: 36px;
        padding-bottom: 36px;
        border-bottom: 1px dotted #aaa;
        &:last-child {
            border: none;
        }
        h2, .h2, h3, .h3 {
            padding-top: 0;
						text-transform: none;
        }
        .listing-content {
            font-size: 0.9em;
            
        }
				.collection-product-action {
					padding-top: 8px;
				}
        .image-wrapper {
            overflow: hidden;
						max-width: 300px;
						margin: 0 0 12px;
            display: block;
                           
           
          
            position: relative;
            .image-overlay {
                @include transition(0.75s);
                
                position: absolute;
                left: 0;
                top: 0;
                display: flex;                
                color: #fff;
                align-items: center;
                justify-content: center;
                width: 100%;
                height: 100%;
                button {
                    @include opacity(0);
                }
                
                
            }
            img {
                width: 100%;                
                @include transition(0.5s);
                @include transform-scale(1,1);                    
    
            }            
            &:hover {
                .image-overlay {
                    background-color: rgba(0,0,0,0.0);
                    @include transition(0.75s);
                    &:after {                       
                        font-family: FontAwesome;                       
                    }
                    &.has-video:after {
                       
                    }
                button {
                    @include opacity(0);
                }                    
                }
                img {                     
                    @include transition(0.75s);
                    @include transform-scale(1.1,1.1);     
                }
            }
        }

    }
}


