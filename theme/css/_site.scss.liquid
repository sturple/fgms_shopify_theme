
/***************** SITE CSS *************************************/


html {}
body {	
	$body-font-size: 18px;
	position: relative;
	font-family: Roboto, sans-serif;
	color: $body-color;
	line-height: 1.56em;	
	font-size: ($body-font-size * 0.9); 
	font-weight: 400;

    &.fonts-loaded{ p, ul, h1, h2, h3, h4, h5, a,i, blockquote {@include opacity(1);}}
	@media  screen and (min-width: $screen-lg-max) {
		font-size: $body-font-size; 
	}		
}


h1, h2, h3, h4, h5, p, ul, ol{
	margin: $reset-margin;
} 

h1, h2, h3, h4, h5,.h1,.h2,.h3 {
	margin: $headings-margin;
	font-weight: 400;
	letter-spacing: -0.02em;

}
.page-frontpage {
	h2, h3, h4, h5,.h2,.h3 , .h4 {
		&:after {
			content : ' ';
			max-width: 80px;
			padding-top: 18px;
			display: block;
			border-bottom: 3px solid #7BC143;
		}		
	}
}


h1, .h1  {
	$h1-font-size: 2.8em;
  font-size: ($h1-font-size * 0.5); 
	
  line-height: 1em;
	margin: 0 0 36px;
  text-transform: uppercase;
	color: $h1-color;
	&:after {
		content : ' ';
		max-width: 80px;
		padding-top: 18px;
		display: block;
		border-bottom: 3px solid #7BC143;
	}	
	
	&.blog-fake-heading {
		font-size: $h1-font-size / 2;
		margin: 0;
		&:after {
			padding: 0;
			border: none;
			content: '';
			max-width: auto;
		}
	}
	@media  screen and (min-width: $screen-sm-max) {
		font-size: ($h1-font-size * 0.7); 
	}

	@media  screen and (min-width: $screen-lg-max) {
		font-size: $h1-font-size; 
	}	
}

h1.home-page {
	
}
.h1-pre, .h1-sub {
	display: block;
}

.h1-pre {
	@include font-size($h1-font-size, 0.6);
}

.h1-sub {
	@include font-size($h1-font-size, 0.6);
}

h2,.h2 { 
	$h2-font-size: 2.2em;
	padding: 24px 0 0;;
	margin-bottom: 24px;  
 
	color: $h2-color;
	font-size: ($h2-font-size * 0.7);
	
	@media  screen and (min-width: $screen-lg-max) {
		font-size: $h2-font-size; 
	}
	
}

h2.tab, h3.tab, h4.tab {
	padding-top: 0;
}

h3,.h3 { 
	font-size: 1.6em;
	padding: $h3-padding;
	color: $h3-color;
	
	
}


h4,.h4 { 
	@include font-size($h4-font-size); 
	padding: $h4-padding;
	color: $h4-color;
}

h5,.h5 { 
	@include font-size($h5-font-size);  
	padding: $h5-padding;
	color: $h5-color;
}
.fa-ul {
	padding-top: 24px;
	margin-bottom: 32px;
	li {
		margin-bottom: 6px;
	}
}

p {
	margin-bottom :$p-margin-bottom ;
	letter-spacing: 0;
}


blockquote ,h1.blockquote, h2.blockquote, .archive-meta p, .intro-text {
	$block-quote-font-size: 1.4em;
	font-size: $block-quote-font-size * 0.8;
	
	line-height: $blockquote-line-height;	
	border: 0; 
	padding: 0; 
	text-transform: none;
	margin-bottom: 36px;
	font-size: ($block-quote-font-size * 0.7); 
	@media  screen and (min-width: $screen-lg-max) {
		font-size: $block-quote-font-size; 
	}	
 
}
.archive-meta p {
    margin: 0 0 24px;
}
h1.blockquote{margin-bottom: 36px;}
blockquote p {  margin: 0 0 6px;  padding: 0;}
blockquote .cite {  @include font-size(1.4); margin: 0;}
*:focus,a:focus,.btn:focus, .btn:active:focus, .btn.active:focus, .btn.focus, .btn.focus:active, .btn.active.focus{outline: none;}
.align-right-lg, .align-right-md, .align-right-sm, .align-right-xs {text-align: right;} 
.footer-form .error-message, .footer-form .input-error-message {color: #fff;}
.error-message.show-error-message , .input-error-message.show-error-message{display: block; }
.error-message, .input-error-message {font-size: 13px; font-size: 1.3rem; color: #990000;  display: none; padding-left: 2px;}
.has-error {
   input, textearea, select {background-color: #FBE3E4;}
}
ul {list-style: none;padding:0;margin:0;}

.line-separator-bottom {border-bottom: 1px dotted #ccc; margin-bottom: 28px !important; padding-bottom: 28px !important;}
.no-line-separator{ margin-bottom: 12px}
.separator {
	margin: 0 4px;
	&:after {
		content: "";
		background-image: url({{ 'green-bullet.png' | asset_url }});
		background-repeat: no-repeat;
		background-position: 0 0;
		
		width: 13px;
		height: 13px;
		top: 0;
		left: 0;
		display: inline-block;
		
	}
}
/*
a, a:visited, a:focus, #recaptcha_privacy a , #recaptcha_privacy a:visited, #recaptcha_privacy a:focus,  .panel-title > a  {	color: $link-color ;	text-decoration: none;}
a:hover , #recaptcha_privacy a:hover, .panel-title > a:hover{color: $link-color-hover;	text-decoration: none;}
*/

@include a-border($link-color,transparent, $link-color-hover, $link-color-hover,'',":not(.btn):not(.icons):not(.logo):not(.subnav):not(.photo):not(.sz-close):not(.sz-left):not(.sz-right):not(.sz-zoomed)");
	@media screen and (min-width: $screen-xs-max) {
		body {
			font-size : $body-font-size-xs-px;
		}	
		
	} 
	@media screen and (min-width: $screen-sm-max) {
		body {
			font-size : $body-font-size-sm-px;
		}	
	
			
	}
	@media screen and (min-width: $screen-md-max) {
		body {
			font-size : $body-font-size;
		}	
	}
	@media screen and (min-width: $screen-lg-max) {

	}

.siteby {
	.separator { 
		display: none; 
		visibility: hidden;
	}	
	@media screen and (min-width: $screen-xs-max) {
		display: inline;
		.separator { 
			display: inline; 
			visibility: visible;
		}
	
		
	} 	
}
 


.pagination-list {
	list-style: none;
	
	padding: 0;
	li {
		margin: 4px 2px;
		display: inline;
		@include a($link-color, none, $link-color-hover) ;
		&.active {
			a {
				color: $link-color-hover;
				span {
					font-weight: bold;
				}
			}
		}
	}
	
	.fa-3x {
		font-size: 1.2em;
	}	
	.fa-2x {
		font-size: 1em;
	}
	.fa-stack-1x {
		font-size: 0.8em;
	}
}
/********** Custom Bullets ******************/

.has-underline {
	border-bottom: 1px solid $list-boarder-color;	
	padding-bottom: 24px !important; 
	margin-bottom: 38px !important;
}

ul {
		

	
	&.B_crumbBox {
		i {
			font-size: 0.8em;
			position: relative;
			top: 1px;
			left: 2px;
		}
		line-height: 1em; padding: 0;
		li {  display: inline;  font-size: 11px; font-size: 1.1rem;  list-style-type: none;  text-transform: none;  padding: 0; margin: 0; }
	}


}
ol {margin-bottom: 24px; padding: 12px 0 0 24px; line-height: 1.6em;
	li {
		margin: 0 12px 12px 16px;
		padding-left: 12px;
		ul {
			margin-top: 10px;
			li { list-style-image: none; margin: 0 0 10px 26px; padding-left: 6px; }
		}
	}
}

.page-content {	
	ul:not('.index-container') {
		margin-left: 20px;
		li:not('.article-row') {
			list-style-image: url({{ 'green-bullet.png' | asset_url }}) ;
			padding: 0  0 8px 8px;
		}
	}
}


/************** Containers *********/
#outer-container { 
	margin: 0 auto;	
	position: relative;  
	width: 100%;  
	z-index: 2;
	background-color: transparent;
	.contentmain-outer-wrapper {
		padding: 0 8px;
	}
	@media screen and (min-width: $screen-sm-max) {
		.contentmain-outer-wrapper {
			padding: 0;
		}		
	}
}
.column-wrapper {

    
}







.icon-your-cart i {margin-right: 6px; font-size: 24px;}
.icon-your-account i {margin-right: 6px; font-size: 24px;}



.embed-responsive {
  position: relative;
  display: block;
  height: 0;
  padding: 0;
  overflow: hidden;
}
.embed-responsive .embed-responsive-item,
.embed-responsive iframe,
.embed-responsive embed,
.embed-responsive object,
.embed-responsive video {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100%;
  border: 0;
}
.embed-responsive-16by9 {
  padding-bottom: 56.25%;
}
.embed-responsive-4by3 {
  padding-bottom: 75%;
}
*[data-background-lg], *[data-background-md], *[data-background-sm], *[data-background], *[data-background]:before, .background-class {
    background-repeat: repeat-y;
    background-position: top left;
    background-attachment: fixed;
    background-size:  100% auto;
	height: 100%;
	
	&.feature-background {
		
	}
}

*[data-background]:before {
	content: " " ;
}

.touch {
	*[data-background-lg], *[data-background-md], *[data-background-sm], *[data-background], *[data-background]:before, .background-class {
			background-attachment: scroll;	
	}
}


/*1200*/
@media screen and (max-width: $screen-lg-max) {
	.align-right-lg {text-align: left;}
	/** Footer **/
	#socialLinks {  padding: 12px 0;}
	#paymentMethods { text-align: left;}
	.payment-column-special-notes-column{padding-left: 36px;}
}
/*980*/
@media screen and (max-width: $screen-md-max) {
	body , .form-control{font-size: 15px;}
	#contentWrapper {margin: 18px 0 42px;}
	.header-align-right, #imageLogo ,.header-cart-bar{  text-align: center;}	
	.align-right-md {text-align: left;}
}
	
/*768*/
@media screen and (max-width: $screen-sm-max) {
	.control-label {margin-bottom: 4px; padding-left: 19px;}
	.nav-tabs.accordion { display:none; }
	.tab-content.accordion .tab-accordion-header {
		display:block;
		border:1px solid #f5f4f3;
		padding:8px 12px;
		margin-top:4px;
		webkit-box-shadow:1px 1px 2px rgba(0,0,0,0.15);
		-moz-box-shadow:1px 1px 2px rgba(0,0,0,0.15);
		box-shadow:1px 1px 2px rgba(0,0,0,0.15);
	}
	.hidden-768 {display: none; visibility: hidden;}
  	.tab-content.accordion .tab-pane {border:1px solid #ccc; padding:24px 12px;	background-color:transparent;	}align-right-sm {text-align: left;}	
	.btn-lg, .btn-group-lg > .btn {
		@include font-size(1.8);		
		padding: 12px 36px;
	  }
	.table-responsive {  margin-bottom: 36px;}

	
}
/*480*/
@media screen and (max-width: $screen-xs-max) {
	.display-block-480 {display: block; margin-bottom: 4px;}
	.display-none-480 {display: none; visibility: hcdidden;}
	.align-right-xs {text-align: left;}
	.pill-wrapper.form-selector .btn {padding: 4px 6px;  }
	.social-stream-wrapper { margin: 0 auto; width: 268px; }
	#social-stream { padding: 24px;}
	.dcsns-toolbar {  overflow: visible;}
	.dcsns-bottom-shadow {
		background: url({{'background-social-feeds-shadow.gif' | asset_url}}) no-repeat center bottom;; 	  
		bottom: 29px;		
		height: 11px;
		left: 19px;
		position: absolute;
		width: 93%;
	}
		
			
	
}