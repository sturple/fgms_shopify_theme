{% if type == blank %}
<nav class="{{class}} social-icons">
	{% unless settings.social-links-facebook == '' %}<a href="{{ settings.social-links-facebook }}" title="{{ shop.name }} on Facebook" target="_blank"><i class="fa fa-facebook"></i></a>{% endunless %}
	{% unless settings.social-links-twitter == '' %}<a href="{{ settings.social-links-twitter }}" title="{{ shop.name }} on Twitter" target="_blank"><i class="fa fa-twitter"></i></a>{% endunless %}
	{% unless settings.social-links-google-plus == '' %}<a href="{{ settings.social-links-google-plus }}" title="{{ shop.name }} on Google Plus" target="_blank"><i class="fa fa-google-plus"></i></a>{% endunless %}
	{% unless settings.social-links-youtube == '' %}<a href="{{ settings.social-links-youtube }}" title="{{ shop.name }} on Youtube" target="_blank"><i class="fa fa-youtube-square"></i></a>{% endunless %}
	{% unless settings.social-links-flickr == '' %}<a href="{{ settings.social-links-flickr }}" title="{{ shop.name }} on Flickr" target="_blank"><i class="fa fa-flickr"></i></a>{% endunless %}

	{% unless settings.social-links-pinterest == '' %}<a href="{{ settings.social-links-pinterest }}" title="{{ shop.name }} on Pinterest" target="_blank"><i class="fa fa-pinterest"></i></a>{% endunless %}
	{% unless settings.social-links-instagram == '' %}<a href="{{ settings.social-links-instagram }}" title="{{ shop.name }} on Instagram" target="_blank"><i class="fa fa-instagram"></i></a>{% endunless %}
	{% unless settings.social-links-linkedin == '' %}<a href="{{ settings.social-links-linkedin }}" title="{{ shop.name }} on LinkedIn" target="_blank"><i class="fa fa-linkedin"></i></a>{% endunless %}
	{% unless settings.social-links-rss == '' %}<a href="{{ settings.social-links-rss }}" title="{{ shop.name }} rss" target="_blank"><i class="fa fa-rss"></i></a>{% endunless %}
</nav><!-- socialLinks -->
{% elsif type == 'sprite'%}
	<div style="margin-bottom: 24px; padding-top: 34px;">
		<div class="socialBar"  >
			<ul >                                                
				{% unless settings.social-links-facebook == '' %}<li class="pull-left">
					<a href="{{ settings.social-links-facebook }}" target="_blank"  title="{{ shop.name }} on Facebook">
						<div class="fgms-social-facebook-feed"></div>
						<div class="social-icon-text"></div>
					</a>                                                
				</li>{% endunless %}
				{% unless settings.social-links-twitter == '' %}<li class=" pull-left"><a href="{{ settings.social-links-twitter }}" target="_blank" title="{{ shop.name }} on Twitter"><div class="fgms-social-twitter"></div><div class="social-icon-text"></div></a></li>{% endunless %}
				{% unless settings.social-links-google-plus == '' %}<li class=" pull-left"><a href="{{ settings.social-links-google-plus }}" rel="publisher" target="_blank"  title="{{ shop.name }} on Google Plus"><div class="fgms-social-googlePlus"></div><div class="social-icon-text"></div></a></li>{% endunless %}
				{% unless settings.social-links-youtube == '' %}<li class=" pull-left"><a href="{{ settings.social-links-youtube }}" target="_blank" title="{{ shop.name }} on Youtube" ><div class="fgms-social-youtube"></div><div class="social-icon-text"></div></a></li>{% endunless %}
				{% unless settings.social-links-pinterest == '' %}<li class=" pull-left"><a href="{{ settings.social-links-pinterest }}" target="_blank"  title="{{ shop.name }} on Pinterest" ><div class="fgms-social-pinterest"></div><div class="social-icon-text"></div></a></li>{% endunless %}
				{% unless settings.social-links-flickr == '' %}<li class=" pull-left"><a href="{{settings.social-links-flickr}}" target="_blank"  title="{{ shop.name }} on Flickr"><div class="fgms-social-flickr"></div><div class="social-icon-text"></div></a></li>{% endunless %}
				{% unless settings.social-links-instagram == '' %}<li class=" pull-left"><a href="{{ settings.social-links-instagram }}" target="_blank" title="{{ shop.name }} on Instagram" ><div class="fgms-social-instagram"></div><div class="social-icon-text"></div></a></li>{% endunless %}
				{% unless settings.social-links-rss == '' %}<li class=" pull-left"><a href="{{ settings.social-links-rss }}" target="_blank" title="{{ shop.name }} rss" ><div class="fgms-social-rss-feed"></div><div class="social-icon-text"></div></a></li>{% endunless %}
			 </ul>
			<div style="clear: both;" class="clearfix"></div>
		</div>
							
	</div>	
{%endif%}