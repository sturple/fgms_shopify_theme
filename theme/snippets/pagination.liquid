{% if paginate.pages > 1 %}
<section id="pagination" class="section-wrap">
  <div class="row">
    <div class="col-xs-12">
      <ul class="pagination-list">        
        {% if paginate.previous %}
        <li>
          <a href="{{paginate.previous.url}}" class="icons" >
            <span class="fa-stack fa-3x">
              <i class="fa fa-square-o fa-stack-2x"></i>
              <i class=" fa-stack-1x fa fa-caret-left" style="position: relative; top: -2px;" aria-hidden="true"></i>
            </span> 
          </a>
        </li>
        {% endif %} 
        
        {% for p in paginate.parts %} 
          {% if p.is_link or p.title == paginate.current_page %}
          <li {% if p.title == paginate.current_page %} class="active" {% endif %} >          
            <a href="{{ p.url }}" class="icons">
              <span class="fa-stack fa-3x">
                <i class="fa fa-square-o fa-stack-2x"></i>
                <span class="fa-stack-1x calendar-text" style="">{{ p.title }}</span>
              </span>            
            </a>
          </li>

          {% else %}
          <li class="disabled">{{ p.title | link_to: p.url }}</li>
          {% endif %} 
        {% endfor %}
        
        {% if paginate.next %}
        <li>
       
          <a href="{{paginate.next.url}}" class="icons" >
            <span class="fa-stack fa-3x">
              <i class="fa fa-square-o fa-stack-2x"></i>
              <i class=" fa-stack-1x fa fa-caret-right" style="position: relative; top: -2px;" aria-hidden="true"></i>
            </span> 
          </a>
        </li>
        {% endif %}
        
      </ul>
    </div>
  </div>
</section>
{% endif %}