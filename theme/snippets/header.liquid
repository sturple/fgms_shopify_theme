<div  id="header-top-nav-parent" >{% include 'nav_account_cart' fullBar : true %}</div>
<div>
	<div id="header-top-parent" class="full-wrapper script-navigation-off" >
		<div id="header-wrapper">
			<div class="row" style="position: relative; display:flex;">
				<!-- header -->
				<div class="header-logo col-md-3">
					<a href="{{settings.main-website}}">
						<span class="sr-only">{{ shop.name }}</span>
						<img src="{{'branding-header-logo-image-file.png' | asset_url}}" alt="{{shop.name}} Logo" class="{{settings.branding-header-logo-icon-class}} " style="" />		        
					</a>
				</div>
				<!-- navigation -->
				<div id="navigation-wrapper" class="col-md-9 align-right-md">
					{% if settings.layout-utility-bar  == false %}<div class="header-utility-wrapper">{% include 'nav_account_cart'%} </div>{%endif%}
					<div class="header-nav-wrapper">{% include 'navbar' linklist : settings.navigation-linklist-main, type : 'main' %}</div>				
					<div class="clearfix" style="clear:both"></div>				
				</div>              
			</div>	
			
		</div>	
	</div>	
</div>




 